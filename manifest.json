{
  "name": "Save Code",
  "version": "0.9.4",
  "description": "Save & organize code snippets that work",
  "permissions": ["declarativeContent", "activeTab"],
  "background": {
    "scripts": ["app/js/background.js", "app/js/popup.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["app/js/content.js"],
    "exclude_matches": ["*://www.thiscodeworks.com/*"]
  }],
  "page_action": {
    "default_popup": "app/popup.html",
    "default_icon": {
      "16": "app/images/tcw_16.png",
      "32": "app/images/tcw_32.png",
      "48": "app/images/tcw_48.png",
      "128": "app/images/tcw_128.png"
    }
  },
  "icons": {
    "16": "app/images/tcw_16.png",
    "32": "app/images/tcw_32.png",
    "48": "app/images/tcw_48.png",
    "128": "app/images/tcw_128.png"
  },
  "web_accessible_resources": [
    "app/images/saveicon.png"
   ],
  "manifest_version": 2
}
